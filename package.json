{
  "name": "tinder-clone",
  "version": "1.0.0",
  "scripts": {
    "build": "etsc --config=etsc.config.js",
    "test": "dotenv-load jest --detectOpenHandles --coverage --verbose --testTimeout=30000",
    "dev": "nodemon --watch . -e ts,tsx --exec ts-node ./src",
    "start": "ts-node ./src",
    "generate-joi-interface": "ts-node ./joi-to-ts-interface.ts",
    "lint": "eslint src/**/*.ts",
    "format": "eslint --fix src/**/*.ts",
    "prisma-seed": "dotenv-load ts-node ./prisma/seeders/index.ts",
    "prisma-generate": "dotenv-load prisma generate",
    "prisma-migrate-deploy": "dotenv-load prisma migrate deploy && prisma generate",
    "prisma-migrate-dev-create": "dotenv-load prisma migrate dev --create-only --name"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "Tinder Clone",
  "devDependencies": {
    "@eslint/config-inspector": "^0.5.6",
    "@eslint/js": "^9.14.0",
    "@jest/globals": "^29.7.0",
    "@jest/types": "^29.6.3",
    "@types/eslint-config-prettier": "^6.11.3",
    "@types/eslint__js": "^8.42.3",
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.12",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/multer": "^1.4.12",
    "@types/node": "^22.8.7",
    "@typescript-eslint/parser": "^8.13.0",
    "dotenv-load": "^3.0.0",
    "esbuild": "^0.24.0",
    "esbuild-node-tsc": "^2.0.5",
    "eslint": "^9.14.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "globals": "^15.12.0",
    "jest": "^29.7.0",
    "jest-junit": "^16.0.0",
    "jest-mock-extended": "2.0.4",
    "joi-to-typescript": "^4.14.0",
    "prettier": "^3.3.3",
    "prisma": "^5.21.1",
    "rimraf": "^6.0.1",
    "ts-jest": "^29.2.3",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.6.3",
    "typescript-eslint": "^8.13.0"
  },
  "dependencies": {
    "@joi/date": "^2.1.1",
    "@prisma/client": "^5.21.1",
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.17",
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "express": "^4.21.1",
    "joi": "^17.13.3",
    "jsonwebtoken": "^9.0.2",
    "multer": "1.4.5-lts.1",
    "redis": "^4.7.0",
    "uuid": "^11.0.2"
  },
  "prisma": {
    "seed": "node ./prisma/seeders/index.js"
  }
}
